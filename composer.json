{
    "name": "crixuamg/laravel-decorators",
    "description": "",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "decorator",
        "pattern"
    ],
    "authors": [
        {
            "name": "Christian Kaal",
            "email": "christiankaal@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.8.0||6.*"
    },
    "require-dev": {
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "3.*",
        "friendsofphp/php-cs-fixer": "v2.*",
        "barryvdh/laravel-ide-helper": "^2.5",
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench": "^3.6",
        "nunomaduro/larastan": "^0.3.4",
        "mockery/mockery": "^1.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CrixuAMG\\Decorators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrixuAMG\\Decorators\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "php-cs-fixer": "php-cs-fixer fix --rules=@PSR2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrixuAMG\\Decorators\\DecoratorServiceProvider"
            ]
        }
    }
}
