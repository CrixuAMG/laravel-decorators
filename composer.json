{
  "name": "crixuamg/laravel-decorators",
  "description": "",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "crixuamg",
    "laravel",
    "decorator",
    "pattern"
  ],
  "authors": [
    {
      "name": "Christian Kaal",
      "email": "christiankaal@gmail.com"
    }
  ],
  "require": {
    "php": "*",
    "illuminate/contracts": "*",
    "illuminate/console": "*",
    "illuminate/database": "*",
    "illuminate/http": "*",
    "illuminate/validation": "*",
    "illuminate/pagination": "*",
    "illuminate/support": "*",
    "crixuamg/laravel-responsable": "0.*"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.13",
    "squizlabs/php_codesniffer": "^3.7",
    "friendsofphp/php-cs-fixer": "^3.14",
    "barryvdh/laravel-ide-helper": "^2.13",
    "phpunit/phpunit": "^9.6",
    "orchestra/testbench": "^7.22",
    "mockery/mockery": "^1.5",
    "nunomaduro/phpinsights": "^2.7"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CrixuAMG\\Decorators\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CrixuAMG\\Decorators\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "php-cs-fixer": "php-cs-fixer fix --rules=@PSR2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CrixuAMG\\Decorators\\DecoratorServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
